<Application x:Class="SmartLogReader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="WindowMain.xaml">
    <Application.Resources>
      <!-- -->
      <SolidColorBrush x:Key="{x:Static SystemColors.HighlightBrushKey}" Color="#FF2288FF"/>
      <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="#FF77BBFF"/>
      <!-- -->
      <LinearGradientBrush x:Key="linearGray" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#FFFFFFFF"/>
        <GradientStop Offset="0.5" Color="#FFEBEBEB"/>
        <GradientStop Offset="0.5" Color="#FFDCDCDC"/>
        <GradientStop Offset="1" Color="#FFD0D0D0"/>
      </LinearGradientBrush>
      <!-- -->
    </Application.Resources>
</Application>
